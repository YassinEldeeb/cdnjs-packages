  "homepage": "https://github.com/dotansimha/graphql-yoga",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-yoga.git"
  },
  "authors": [
    {
      "name": "@dotan",
      "email": "dotansimha@gmail.com"
    },
    {
      "name": "@saihaj",
      "email": "saihajpreet.singh@gmail.com"
    }
  ],
  "autoupdate": {
    "source": "npm",
    "target": "graphql-yoga",
    "fileMap": [
